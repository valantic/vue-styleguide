{
  "name": "@valantic/vue-styleguide",
  "version": "0.0.4",
  "description": "A demo tool to test a vue project locally.",
  "type": "module",
  "lastDependencyUpdate": "01.10.2025",
  "scripts": {
    "release": "npm version patch -m 'Release %s' && git push --follow-tags",
    "release:minor": "npm version minor -m 'Release %s' && git push --follow-tags",
    "release:major": "npm version major -m 'Release %s' && git push --follow-tags",
    "clean:caches": "rm -rf .eslintcache .stylelintcache node_modules/.cache",
    "dev": "vite",
    "fix:stylelint": "npm run lint:stylelint -- --cache=false --config .stylelintrc.fix.js --fix",
    "lint": "npm run lint:eslint && npm run lint:stylelint && npm run tsc",
    "lint:eslint": "eslint --ext .ts,.js,.vue ./",
    "lint:stylelint": "stylelint 'src/**/*.?(vue|scss)'",
    "lint-staged": "lint-staged",
    "test": "npm run lint && npm run test:unit -- --watch=false",
    "test:unit": "vitest --dir tests",
    "build:watch": "npm run build -- --watch",
    "serve": "vite preview",
    "prepare": "husky",
    "prettier": "prettier . --write",
    "tsc": "vue-tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/valantic/vue-styleguide.git",
    "tree": "https://github.com/valantic/vue-styleguide/tree/v"
  },
  "engines": {
    "node": ">=22 <25",
    "npm": ">=10 <12"
  },
  "keywords": [
    "vue",
    "vuejs",
    "valantic"
  ],
  "author": "valantic CEC",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/valantic/vue-styleguide/issues"
  },
  "homepage": "https://github.com/valantic/vue-styleguide",
  "dependencies": {
    "vue": "~3.5.28"
  },
  "devDependencies": {
    "@eslint/eslintrc": "~3.3.1",
    "@eslint/js": "~9.36.0",
    "@trivago/prettier-plugin-sort-imports": "~5.2.2",
    "@types/node": "~24.6.1",
    "@vitejs/plugin-vue": "~6.0.1",
    "@vue/test-utils": "~2.4.6",
    "autoprefixer": "~10.4.21",
    "cssnano": "~7.1.1",
    "eslint": "~9.36.0",
    "eslint-config-prettier": "~10.1.8",
    "eslint-config-valantic": "~18.0.0",
    "eslint-import-resolver-typescript": "~4.4.4",
    "eslint-import-resolver-vite": "~2.1.0",
    "eslint-plugin-import": "~2.32.0",
    "eslint-plugin-jsdoc": "~60.7.0",
    "eslint-plugin-unicorn": "~61.0.2",
    "eslint-plugin-vue": "~10.5.0",
    "globals": "~16.4.0",
    "husky": "~9.1.7",
    "jsdom": "~27.0.0",
    "lint-staged": "~16.2.7",
    "postcss": "~8.5.6",
    "postcss-html": "~1.8.0",
    "postcss-scss": "~4.0.9",
    "prettier": "~3.6.2",
    "sass": "~1.93.2",
    "stylelint": "~16.24.0",
    "stylelint-config-valantic": "~9.1.0",
    "svgo": "~4.0.0",
    "the-new-css-reset": "~1.11.3",
    "typescript": "~5.9.3",
    "typescript-eslint": "~8.45.0",
    "vite": "~7.1.8",
    "vite-plugin-image-optimizer": "~2.0.2",
    "vite-plugin-markdown": "~2.2.0",
    "vitest": "~3.2.4",
    "vue-component-type-helpers": "~3.1.0",
    "vue-eslint-parser": "~10.2.0",
    "vue-router": "~4.5.1",
    "vue-tsc": "~3.1.0"
  },
  "browserslist": [
    "> 2%",
    "last 2 years",
    "not ie_mob > 0",
    "not ie > 0",
    "not dead"
  ],
  "msw": {
    "workerDirectory": "public"
  },
  "files": [
    "src"
  ]
}
